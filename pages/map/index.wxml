<!--pages/bless/index.wxml-->
<view class="container">

  <view class="map">
    <map id="map" longitude="{{lng}}" latitude="{{lat}}" scale="18" controls="{{controls}}" markers="{{markers}}" bindmarkertap="markertap" show-location style="width: 750rpx; height: 90vh"></map>
  </view>
  <view class="bottomImg">
    <image src="/images/map_bottom_new.png" mode="widthFix" />
  </view>
</view>